import java.util.Scanner; 


public class Main
{
   static boolean Prime(int a)
   {
        //create a loop from 2 to check the prime or not
        for(int i=2; i<a/2; i++)
        {
            if(a%i==0)
                return false;
            
            
        }
        return true;
   }
    public static void main(String[] args)
    {
       Scanner a= new Scanner(System.in);
        int x= a.nextInt();
        System.out.println(Prime(x));
        
        
    }
}
