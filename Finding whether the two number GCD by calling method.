
public class Main
{
    //without puting static in the method.
     int gcd(int m, int n){
        while(m!=n){
           
                if(m>n){
                    m=m-n;
                }else{
                    n=n-m;
                }
            }
        return m;
    }
	public static void main(String[] args) {
	    Main x= new Main();//by creating a class
		System.out.println(x.gcd(25,15));
	}
}
